{
  "name": "webpack-starter",
  "productName": "Webpack starter",
  "version": "1.0.1",
  "description": "Webpack boilerplate for web apps (w/ Babel & Sass)",
  "author": "Scott Thiessen (http://movingobjects.io)",
  "license": "ISC",
  "homepage": "https://github.com/movingobjects/webpack-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/movingobjects/webpack-starter.git"
  },
  "bugs": {
    "url": "https://github.com/movingobjects/webpack-starter/issues"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "main": "app/src/entry.js",
  "scripts": {
    "build": "node ./scripts/build.js",
    "dev": "node ./scripts/build.js -w",
    "enable-scripts": "node ./scripts/clean -s",
    "reinstall": "node ./scripts/clean.js -m"
  },
  "devDependencies": {
    "ajv": "^6.1.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.9",
    "extract-text-webpack-plugin": "^3.0.2",
    "favicons-webpack-plugin": "0.0.9",
    "file-loader": "^1.1.8",
    "fs-extra": "^5.0.0",
    "html-webpack-plugin": "^2.30.1",
    "log-box": "^0.4.3",
    "minimist": "^1.2.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.20.1",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.2",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {},
  "log-box": {
    "default": {
      "style": "round",
      "color": "#9f0",
      "textColor": "#9f0"
    }
  }
}
